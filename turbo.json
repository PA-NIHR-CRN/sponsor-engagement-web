{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**/*", ".next/**", "!.next/cache/**"]
    },
    "lint": {},
    "lint:fix": {},
    "start": {},
    "test": {},
    "test:ci": {},
    "typecheck": {},
    "dev": {
      "cache": false,
      "persistent": true
    },
    "db:generate": {
      "cache": false
    },
    "db:push": {
      "cache": false
    }
  }
}

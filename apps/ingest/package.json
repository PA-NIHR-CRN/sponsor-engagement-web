{
  "name": "ingest",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "dev": "nodemon --exec \"node -r esbuild-register ./src/index.ts\" -e .ts",
    "start": "node -r esbuild-register ./src/index.ts",
    "test": "jest --watch",
    "test:ci": "jest --ci --coverage"
  },
  "dependencies": {
    "@nihr-ui/logger": "*",
    "axios": "^1.5.0",
    "database": "*",
    "dayjs": "^1.11.10"
  },
  "devDependencies": {
    "@types/node": "^15.12.2",
    "esbuild": "^0.14.38",
    "esbuild-register": "^3.3.2",
    "jest": "^29.7.0",
    "msw": "^1.3.1",
    "nodemon": "^2.0.15",
    "ts-jest": "^29.1.1",
    "ts-mockery": "^1.2.0",
    "tsconfig": "*",
    "tsup": "^8.0.1",
    "typescript": "^4.5.3"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "noExternal": [
      "@nihr-ui/logger"
    ],
    "splitting": false,
    "sourcemap": true,
    "clean": true
  }
}

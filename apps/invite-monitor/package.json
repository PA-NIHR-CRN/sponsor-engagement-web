{
  "name": "invite-monitor",
  "version": "0.0.0",
  "private": true,
  "description": "Montiors the status of invitation emails",
  "scripts": {
    "build": "tsup",
    "dev": "nodemon --exec \"node -r esbuild-register ./src/index.ts\" -e .ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "start": "node -r esbuild-register ./src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@types/node": "^15.12.2",
    "esbuild": "^0.14.38",
    "esbuild-register": "^3.3.2",
    "nodemon": "^3.1.9",
    "tsconfig": "*",
    "tsup": "^8.0.1",
    "typescript": "^4.5.3"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "splitting": false,
    "sourcemap": true,
    "clean": true
  }
}
